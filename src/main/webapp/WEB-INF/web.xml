<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>ManagerLoginServlet</servlet-name>
        <servlet-class>com.ex.project_1.servlets.ManagerLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManagerLoginServlet</servlet-name>
        <url-pattern>/ManagerLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EmployeeLoginServlet</servlet-name>
        <servlet-class>com.ex.project_1.servlets.EmployeeLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmployeeLoginServlet</servlet-name>
        <url-pattern>/EmployeeLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterEmployeeServlet</servlet-name>
        <servlet-class>com.ex.project_1.servlets.RegisterEmployeeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterEmployeeServlet</servlet-name>
        <url-pattern>/RegisterEmployeeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EmployeeRequestServlet</servlet-name>
        <servlet-class>com.ex.project_1.servlets.EmployeeRequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmployeeRequestServlet</servlet-name>
        <url-pattern>/EmployeeRequestServlet</url-pattern>
    </servlet-mapping>





</web-app>
